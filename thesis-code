\[CapitalPi]b1 = 
  s (f\[CapitalUpsilon]^2/(
     m\[CapitalUpsilon]^2 (m\[CapitalUpsilon]^2 - p0))) /. p0 -> s;
\[CapitalPi]b2 = 
  s (f\[CapitalUpsilon]p^2/(
     m\[CapitalUpsilon]p^2 (m\[CapitalUpsilon]p^2 - p0))) /. p0 -> s;

\[CapitalPi]b3 = 
  s (f\[CapitalUpsilon]pp^2/(
     m\[CapitalUpsilon]pp^2 (m\[CapitalUpsilon]pp^2 - p0))) /. p0 -> s;
\[CapitalPi]b4 = 
  s (1/p0 Log[Abs[s0b/(s0b - p0)]] (1/(4 \[Pi]^2)) (1 + \[Alpha]s/\[Pi])) /. 
   p0 -> s;

