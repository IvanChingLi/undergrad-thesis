Coup[x_] := 
 Boole[x == 1] CVL + Boole[x == 2] CVR + Boole[x == 3] gLR + 
  Boole[x == 4] gRL + Boole[x == 5] gLLLL + Boole[x == 6] gLLRR + 
  Boole[x == 7] gRRLL + Boole[x == 8] gRRRR

