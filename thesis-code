Find sum of \[CapitalPi] functions

\[CapitalPi]d = 
  s (f\[Rho]0^2/(2 m\[Rho]0^2 (m\[Rho]0^2 - p0)) + f\[Omega]^2/(
      2 m\[Omega]^2 (m\[Omega]^2 - p0)) + 
      1/p0 Log[Abs[s0d/(s0d - p0)]] (1/(4 \[Pi]^2)) (1 + \[Alpha]s/\[Pi])) /. 
   p0 -> s;

\[CapitalPi]u = 
  s (f\[Rho]0^2/(2 m\[Rho]0^2 (m\[Rho]0^2 - p0)) + f\[Omega]^2/(
      2 m\[Omega]^2 (m\[Omega]^2 - p0)) + 
      1/p0 Log[Abs[s0u/(s0u - p0)]] (1/(4 \[Pi]^2)) (1 + \[Alpha]s/\[Pi])) /. 
   p0 -> s;

\[CapitalPi]s = 
  s (f\[Phi]^2/(m\[Phi]^2 (m\[Phi]^2 - p0)) + 
      1/p0 Log[Abs[s0s/(s0s - p0)]] (1/(4 \[Pi]^2)) (1 + \[Alpha]s/\[Pi])) /. 
   p0 -> s;

\[CapitalPi]c = 
  s (fJ\[CapitalPsi]^2/(mJ\[CapitalPsi]^2 (mJ\[CapitalPsi]^2 - p0)) + 
      f\[CapitalPsi]p^2/(m\[CapitalPsi]p^2 (m\[CapitalPsi]p^2 - p0)) + 
      1/p0 Log[Abs[s0c/(s0c - p0)]] (1/(4 \[Pi]^2)) (1 + \[Alpha]s/\[Pi])) /. 
   p0 -> s;

\[CapitalPi]b = 
  s (f\[CapitalUpsilon]^2/(m\[CapitalUpsilon]^2 (m\[CapitalUpsilon]^2 - p0)) +
       f\[CapitalUpsilon]p^2/(
      m\[CapitalUpsilon]p^2 (m\[CapitalUpsilon]p^2 - p0)) + 
      f\[CapitalUpsilon]pp^2/(
      m\[CapitalUpsilon]pp^2 (m\[CapitalUpsilon]pp^2 - p0)) + 
      f\[CapitalUpsilon]ppp^2/(
      m\[CapitalUpsilon]ppp^2 (m\[CapitalUpsilon]ppp^2 - p0)) + 
      1/p0 Log[Abs[s0b/(s0b - p0)]] (1/(4 \[Pi]^2)) (1 + \[Alpha]s/\[Pi])) /. 
   p0 -> s;

